meta {
  name: Update UOM
  type: http
  seq: 3
}

put {
  url: http://127.0.0.1:8000/api/v1/admin/uoms/update?uom_id=5
  body: formUrlEncoded
  auth: bearer
}

params:query {
  uom_id: 5
}

auth:bearer {
  token: {{access_token}}
}

body:form-urlencoded {
  uom_name: MiliLiter
  uom_short_name: ml
  description: small unit
}

settings {
  encodeUrl: true
  timeout: 0
}
