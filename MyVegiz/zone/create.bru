meta {
  name: create
  type: http
  seq: 2
}

post {
  url: http://127.0.0.1:8000/api/v1/zone/create
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{access_token}}
}

body:multipart-form {
  zone_name: South Ahmedabad
  city: Ahmedabad
  state: Gujarat
  polygon: '''
    [
      {"lat":23.0225,"lng":72.5714},
      {"lat":23.0230,"lng":72.5800},
      {"lat":23.0180,"lng":72.5820},
      {"lat":23.0225,"lng":72.5714}
    ]
  '''
  ~is_deliverable: true
}

settings {
  encodeUrl: true
  timeout: 0
}
